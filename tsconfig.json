{
   "compilerOptions": {
      "target": "ES2022",
      "module": "CommonJS",
      "lib": ["ES2022"],
      "outDir": "dist",
      "rootDir": "src",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
   },
   "include": ["src/**/*"]
}
